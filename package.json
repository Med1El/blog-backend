{
  "name": "blog-backend",
  "version": "1.0.0",
  "description": "A blog application backend built with Node.js, Express, Mongoose, and TypeScript.",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "node --watch -r ts-node/register --env-file=.env src/app.ts",
    "watch": "ts-node-dev src/app.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "multer": "^1.4.5-lts.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^1.4.12",
    "@types/node": "^14.14.31",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "keywords": [
    "blog",
    "nodejs",
    "express",
    "mongoose",
    "typescript"
  ],
  "author": "Med ALAMI",
  "license": "MIT"
}
